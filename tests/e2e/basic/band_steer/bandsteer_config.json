{
	"uuid": 2,
	"radios": [
		{
			"band": "5G",
			"country": "CA",
			"channel-mode": "HE",
			"channel-width": 80
		},
		{
			"band": "2G",
			"country": "CA",
			"channel-mode": "HE",
			"channel-width": 80
		}
	],
	"interfaces": [
		{
			"name": "WAN",
			"role": "upstream",
			"ethernet": [
				{
					"select-ports": [
						"WAN*"
					]
				}
			],
			"ipv4": {
				"addressing": "dynamic"
			},
			"ssids": [
				{
					"name": "bandsteering_2",
					"wifi-bands": [
						"5G",
						"2G"
					],
					"bss-mode": "ap",
					"encryption": {
						"proto": "psk2",
						"key": "password@123",
						"ieee80211w": "optional"
					},
					"roaming": true,

					"services": [ "wifi-steering" ]
				}
			]
		},
		{
			"name": "LAN",
			"role": "downstream",
			"services": [ "ssh" ],
			"ethernet": [
				{
					"select-ports": [
						"LAN*"
					]
				}
			],
			"ipv4": {
				"addressing": "static",
				"subnet": "192.168.1.1/24",
				"dhcp": {
					"lease-first": 10,
					"lease-count": 100,
					"lease-time": "6h"
				}
			}
		}
	],
	"metrics": {
		"statistics": {
			"interval": 120,
			"types": [ "ssids", "lldp", "clients" ]
		},
		"health": {
			"interval": 120
		}
	},
	"services": {
		"wifi-steering": {
			"mode": "local",
			"network": "upstream",
			"assoc-steering": true,
			"required-snr": -85,
			"required-probe-snr": -80,
			"required-roam-snr": -80,
			"load-kick-threshold": 90
		},
		"ssh": {
			"port": 22
		}
	}
}